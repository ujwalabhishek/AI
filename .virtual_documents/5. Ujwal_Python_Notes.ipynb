def calculate_bill_amount(units):
    billamt = 0
    if units <= 100:
        billamt = units * 60
    elif units <= 300:
        billamt = 100 * 60 + (units-100) * 70
    elif units > 300:
        billamt = 100 * 60 + 200 * 70 + (units-300) * 80
    return billamt

