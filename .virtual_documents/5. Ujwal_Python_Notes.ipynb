def handleException(func):
    def wrapper():
        try:
          func()
        except TypeError:
          print('Ther was a Type error !')
        except ZeroDivisionError:
          print('Ther was a Zero Division error !')
        except Exception:
          print('There was some error !')
    return wrapper

